@model KRBAccounting.Web.ViewModels.Report.ReportClassFeeRateFromViewModel
<script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
@using (Html.BeginForm("ClassFeeRateReport", "Report", new { controller = "Report" }, FormMethod.Post, new { @class = "form-horizontal", id = "report-form" }))
{
   
        
    <div class="clearfix">
    </div>
    <div class="lag">
        <label class="label-90">
            Class</label>
        <span class="field">
            @Html.DropDownListFor(m => m.ClassId, Model.ClassList, new { @class = "input-190" })
            @Html.ValidationMessageFor(m => m.ClassId)
        </span>
        <label class="label-90">
            Shift</label>
        <span class="field">
            @Html.DropDownListFor(m => m.ShiftId, Model.ShiftList, new { @class = "input-190" })
            @Html.ValidationMessageFor(m => m.ShiftId)
        </span>
    </div>
    <div class="clearfix">
    </div>
    <div class="lag">
        <label class="label-90">
            Boarder</label>
        <span class="field">
            @Html.DropDownListFor(m => m.BoaderId, Model.BoaderList, new { @class = "input-190" })
            @Html.ValidationMessageFor(m => m.BoaderId)
        </span>
    </div>
}
