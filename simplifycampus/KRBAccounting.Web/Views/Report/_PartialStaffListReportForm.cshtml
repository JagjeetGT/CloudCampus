@model KRBAccounting.Web.ViewModels.Report.ReportStaffListFromViewModel
<script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
@using (Html.BeginForm("StaffListReport", "Report", new { controller = "Report" }, FormMethod.Post, new { @class = "form-horizontal", id = "report-form" }))
{
   
        
    <div class="lag">
        <label class="label-90">
            Gender</label>
        <span class="field">
            @Html.DropDownListFor(m => m.Gender, Model.GenderList, new { @class = "input-190" })
            @Html.ValidationMessageFor(m => m.Gender)
        </span>
        <label class="label-90">
            Religion</label>
        <span class="field">
            @Html.DropDownListFor(m => m.Religion, Model.ReligionList, new { @class = "input-190" })
            @Html.ValidationMessageFor(m => m.Religion)
        </span>
    </div>
       
 
    <div class="clearfix">
    </div>
    <div class="lag">
        <label class="label-90">
            Status</label>
        <span class="field">
            @Html.DropDownListFor(m => m.Status, Model.StatusList, new { @class = "input-190" })
            @Html.ValidationMessageFor(m => m.Status)
        </span>
        <label class="label-90">
            Group</label>
        <span class="field">
            @Html.DropDownListFor(m => m.Group, Model.GroupList, new { @class = "input-190" })
            @Html.ValidationMessageFor(m => m.Group)
        </span>
    </div>
}
