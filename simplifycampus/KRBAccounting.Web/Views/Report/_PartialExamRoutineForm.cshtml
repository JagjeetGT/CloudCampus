@model KRBAccounting.Web.ViewModels.Report.ReportExamRoutineFormViewModel
<script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
@using (Html.BeginForm("ExamRoutineReport", "Report", new { controller = "Report" }, FormMethod.Post, new { @class = "form-horizontal", id = "report-form" }))
{
   
        
    
    <div class="lag">
        <label class="label-90">
            Class</label>
        <span class="field">
            @Html.DropDownListFor(m => m.ClassId, Model.ClassList, new { @class = "input-190" })
            @Html.ValidationMessageFor(m => m.ClassId)
        </span>
        <label class="label-90">
            Exam</label>
        <span class="field">
            @Html.DropDownListFor(m => m.ExamId, Model.ExamList, new { @class = "input-190" })
            @Html.ValidationMessageFor(m => m.ExamId)
        </span>
    </div>
    <div class="clearfix">
    </div>
    <div class="lag">
        <label class="label-90">
            Subject</label>
        <span class="field">
            @Html.DropDownListFor(m => m.SubjectId, Model.SubjectList, new { @class = "input-190" })
            @Html.ValidationMessageFor(m => m.SubjectId)
        </span>
    </div>
}
