@model KRBAccounting.Web.ViewModels.Attendance.DailyAttendanceFormViewModel
@{
    ViewBag.Title = "Attendace Percent";
}
<script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
<style>
    #ledger-list
    {
        background: none repeat scroll 0 0 #FFFFFF;
        border: 1px solid #CCCCCC;
    }
</style>
<div class="clear">
</div>
<div class="lag">
    <span class="stack">
        <label class="label-100">
            From</label>
        <span class="field">@Html.TextBoxFor(x => x.FromDate, new { @class = "fdate" })
        </span></span>
</div>
<div class="clear">
</div>
<div class="lag">
    <span class="stack">
        <label class="label-100">
            To</label>
        <span class="field">@Html.TextBoxFor(x => x.ToDate, new { @class = "edate" })
        </span></span>
</div>
<div class="clearfix">
</div>
<div class="lag">
    <label class="label-90">
        All Department</label>
    <span class="lag">
        @Html.EditorFor(x => x.AllDepartment)
    </span>
</div>
<div class="lag">
    <label class="label-90">
        All Employee</label>
    <span class="lag">
        @Html.EditorFor(x => x.AllEmployee)
    </span>
</div>
<div class="clear">
</div>
