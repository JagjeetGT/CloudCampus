@using KRBAccounting.Enums
@using KRBAccounting.Web.CustomFilters
@model PagedList<List<KRBAccounting.Domain.Entities.ScLibraryMemberRegistration>>
          @{var i = Convert.ToInt32(ViewBag.SnStart);}
<div class="block-fluid table-sorting">
    <table cellpadding="0" cellspacing="0" width="100%" class="listpopup" id="tbl-list">
        <thead>
            <tr>
                <th width="5%">S.N</th>
                <th>
                    Type
                </th>
                <th>
                    Class
                </th>
               
                
               
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr >
                    <td align="center">@i</td>
                    <td class="detail ttLT" title="Type: @item.FirstOrDefault().Type" data-type="@item.FirstOrDefault().Type" data-classId="@item.FirstOrDefault().ClassId" style="cursor: pointer">@item.FirstOrDefault().Type
                    </td>
                    <td >
                        @{
                            var schClass = item.FirstOrDefault().Class;
                        }
                        @if (schClass != null)
                        {
                            @schClass.Description
                        }
                    </td>
                   
                </tr>
                i++;
            }
        </tbody>
    </table>
    <div class="pagination">
        @Html.Raw(Html.AjaxPager(Model, "LibraryMemberRegistrationsList", "Library", new PagerOptions() { PageIndexParameterName = "pageNo", ShowGoButton = false }, new AjaxOptions() { UpdateTargetId = "LibraryMemberRegistrations-list" }))
    </div>
</div>
@*<script type="text/javascript" src="/Scripts/cooperative.pagination-begin-end-request-loader.js">
</script>*@
