@model KRBAccounting.Domain.Entities.ScDivision
<script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
@using (Ajax.BeginForm("AddDivision", "School", new { controller = "School" }, new AjaxOptions { HttpMethod = "Post", OnComplete = "OnComplete" }, new { @class = "form-horizontal from-custom online-form", id = "frm-id" }))
{
    <div>
        @Html.HiddenFor(m => m.Id)
        <div class="lag">
            <label class="label-90">
                Description</label>
            <div class="field">
                @Html.TextBoxFor(m => m.Description, new { @class = "input-253 input", entermsg = "Enter Desc" })
                @Html.ValidationMessageFor(m => m.Description)<span class="loading Desc">
                    <img title="w_loader_gr.gif" src="/Content/themes/img/loaders/w_loader_gr.gif" /></span>
            </div>
        </div>
        <div class="clearfix">
        </div>
        <div class="lag">
            <label class="label-90">
                Percentage From</label>
            <div class="field">
                @Html.TextBoxFor(m => m.PercentageFrom, new { @class = "input-253 input", entermsg = "Enter Percentage" })
                @Html.ValidationMessageFor(m => m.PercentageFrom)
            </div>
        </div>
             <div class="clearfix">
        </div>
        <div class="lag">
            <label class="label-90">
                Percentage To</label>
            <div class="field">
                @Html.TextBoxFor(m => m.PercentageTo, new { @class = "input-253 input", entermsg = "Enter Percentage" })
                @Html.ValidationMessageFor(m => m.PercentageTo)
            </div>
        </div>
        <div class="clearfix">
        </div>
        <div class="lag">
            <label class="label-90">
                Schedule</label>
            <div class="field">
                @Html.TextBoxFor(m => m.Schedule, new { @class = "input-253 input", entermsg = "Enter Schedule" })
                @Html.ValidationMessageFor(m => m.Schedule)
            </div>
        </div>
    </div>  <div class="clearfix">
    </div>
    <div class="lag">
        <label class="label-90">
            Memo</label>
        <div class="field">
            @Html.TextBoxFor(m => m.Memo, new { @class = "input-253 input", entermsg = "Enter Memo" })
            @Html.ValidationMessageFor(m => m.Memo)
        </div>
    </div>
}