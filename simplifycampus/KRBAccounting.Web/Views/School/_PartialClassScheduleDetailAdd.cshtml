@model KRBAccounting.Domain.Entities.ClassScheduleDetail
<script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
@using (Ajax.BeginForm("ClassScheduleDetailAdd", "{controller}", new { controller = "{controller}" }, new AjaxOptions { HttpMethod = "Post", OnComplete = "OnComplete" }, new { @class = "form-horizontal from-custom online-form", id = "form-id" }))
{<span class="stack"><label class="label-90">Day</label><span class="field">@Html.TextBoxFor(m => m.Day, new { @class = "input-100 input", entermsg = "Enter Day"})@Html.ValidationMessageFor(m => m.Day)</span></span><span class="stack"><label class="label-90">StartTime</label><span class="field">@Html.TextBoxFor(m => m.StartTime, new { @class = "input-100 input", entermsg = "Enter StartTime"})@Html.ValidationMessageFor(m => m.StartTime)</span></span><span class="stack"><label class="label-90">EndTime</label><span class="field">@Html.TextBoxFor(m => m.EndTime, new { @class = "input-100 input", entermsg = "Enter EndTime"})@Html.ValidationMessageFor(m => m.EndTime)</span></span><span class="stack"><label class="label-90">ClassScheduleId</label><span class="field">@Html.TextBoxFor(m => m.ClassScheduleId, new { @class = "input-100 input", entermsg = "Enter ClassScheduleId"})@Html.ValidationMessageFor(m => m.ClassScheduleId)</span></span>}