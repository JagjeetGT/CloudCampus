@model KRBAccounting.Domain.Entities.ScProgramMaster
<script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
@using (Ajax.BeginForm("ProgramMasterAdd", "School", new { controller = "School" }, new AjaxOptions { HttpMethod = "Post", OnSuccess = "OnComplete" }, new { @class = "form-horizontal from-custom online-form", id = "form-id" }))
{
    @Html.HiddenFor(x => x.Id)
    <div class="lag">
        <div class="stack">
            <label class="label-90">
                Description</label>
            <div class="field">
                @Html.TextBoxFor(m => m.Description, new { @class = "input-190 input", entermsg = "Enter Description" })
                @Html.ValidationMessageFor(m => m.Description)<span class="loading Description">
                    <img title="w_loader_gr.gif" src="/Content/themes/img/loaders/w_loader_gr.gif" /></span>
            </div>
        </div>
        <div class="stack">
            <label class="label-90">
                Code</label>
            <div class="field">
                @Html.TextBoxFor(m => m.Code, new { @class = "input-100 input", entermsg = "Enter Code" })
                @Html.ValidationMessageFor(m => m.Code)<span class="loading Code">
                    <img title="w_loader_gr.gif" src="/Content/themes/img/loaders/w_loader_gr.gif" /></span>
            </div>
        </div>
    </div>
    <div class="clearfix">
    </div>
    <div class="lag">
        <div class="stack">
            <label class="label-90">
                Dep. Head</label>
            <div class="field">
                @Html.TextBoxFor(m => m.Incharge, new { @class = "input-190 input", entermsg = "Enter Incharge" })
                @Html.ValidationMessageFor(m => m.Incharge)
            </div>
        </div>
    </div>
    <div class="clearfix">
    </div>

    <div class="lag">
        <div class="stack">
            <label class="label-90">
                Faculty Description</label>
            <div class="field">
                @Html.TextBoxFor(m => m.FacultyDescription, new { @class = "input-450  input", entermsg = "Enter FacultyDescription" })
                @Html.ValidationMessageFor(m => m.FacultyDescription)
            </div>
        </div>
        <div class="clear"></div>
        <div class="stack">
            <label class="label-90">
                Level Description</label>
            <div class="field">
                @Html.TextBoxFor(m => m.LevelDescription, new { @class = "input-190 input", entermsg = "Enter LevelDescription" })
                @Html.ValidationMessageFor(m => m.LevelDescription)
            </div>
        </div>
    </div>
    <div class="clearfix">
    </div>
    <div class="lag">
        <div class="stack">
            <label class="label-90">
                University Desc.</label>
            <div class="field">
                @Html.TextBoxFor(m => m.UniversityDescription, new { @class = "input-450 input", entermsg = "Enter UniversityDescription" })
                @Html.ValidationMessageFor(m => m.UniversityDescription)
            </div>
        </div>
    </div> <div class="clearfix">
    </div>
   
    <div class="clearfix">
    </div>
    <div class="lag">
        <div class="stack">
            <label class="label-90">
                Schedule</label>
            <div class="field">
                @Html.TextBoxFor(m => m.Schedule, new { @class = "input-100 input", entermsg = "Enter Schedule" })
                @Html.ValidationMessageFor(m => m.Schedule)
            </div>
        </div>
         @*  <div class="stack">
            <label class="label-90">
                Name of Class
            </label>
            <div class="field">
                @Html.TextBoxFor(m => m.ClassNameCode, new { @class = "input-90 input ", entermsg = "Enter Schedule" })
                @Html.ValidationMessageFor(m => m.ClassNameCode)
            </div>
        </div>*@
        
    </div>
    <div class="clearfix">
    </div>
    <div class="lag">
        
            <label>
                Year
            </label>
            <div class="field">
                @Html.RadioButtonFor(m => m.DurationId, 1, new { @class = "input-90 input ", entermsg = "Enter Schedule" })
                 
               
            </div>
   
            <label >
                Semester
            </label>
            <div class="field">
                @Html.RadioButtonFor(m => m.DurationId, 2, new { @class = "input-90 input ", entermsg = "Enter Schedule" })
           
        </div>
       
            <label >
                Trimester
            </label>
            <div class="field">
                @Html.RadioButtonFor(m => m.DurationId, 3, new { @class = "input-90 input ", entermsg = "Enter Schedule" })
                 
               
        
        </div>
    </div>
    <div class="clearfix"></div>
          <div class="stack">     
        <label class="label-90">
            Total
        </label>
        <div class="field">
            @Html.TextBoxFor(m => m.TotalSem, new { @class = "input-90 input ", entermsg = "Enter Schedule" })
            @Html.ValidationMessageFor(m => m.TotalSem)
        </div>
    </div>
    <div class="lag detailEntryTable" style="height: 400px; width: 100%">
        <table class="table-bordered" id="table-Semester-detail" style="width: 100%">
            <tr>
                <th>
                    S.N.
                </th>
                <th>
                    Semester
                </th>
                <th>
                    Semester Code
                </th>
            </tr>
        </table>
    </div>
    
}
<script type="text/javascript">

     $(document).ready(function () {
     
         $(function () {
           
             var FacultyDescription = [ @foreach (var item in Model.FacultyList)
                                        {
        
                                       <text> "@item",</text>

                                        } ];
             var levelDec = [@foreach (var item in Model.LevelList)
                             {
        
                                       <text> "@item",</text>

                             }
             ];var UniDec = [@foreach (var item in Model.UniversityList)
                             {
        
                                       <text> "@item",</text>

                             }
             ];
            $("#FacultyDescription").autocomplete({
                source: FacultyDescription
            });
             $("#LevelDescription").autocomplete({
                source: levelDec
            }); $("#UniversityDescription").autocomplete({
                source: UniDec
            });
        });

    });
     

  

 
</script>
<style>
    .ui-menu
    {
        z-index: 1003 !important;
    }
</style>
