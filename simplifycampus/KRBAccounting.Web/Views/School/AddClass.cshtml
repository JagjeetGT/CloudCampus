@model KRBAccounting.Domain.Entities.SchClass
<script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
<script src="../../Content/TimePicker/jquery.timepicker.js" type="text/javascript"></script>
<link href="../../Content/TimePicker/jquery.timepicker.css" rel="stylesheet" type="text/css" />
@using (Ajax.BeginForm("AddClass", "School", new { controller = "School" }, new AjaxOptions { HttpMethod = "Post", OnComplete = "OnComplete" }, new { @class = "form-horizontal from-custom online-form", id = "class-frm" }))
{
    @Html.HiddenFor(x => x.Id)
   <div style="width: 500px;">
        <div class="lag">
            <label class="label-160">
                Code</label>
            <span class="field">
                @Html.TextBoxFor(model => model.Code, new { @class = "input input-253" })
                @Html.ValidationMessageFor(model => model.Code)<span class="loading Code">
                    <img title="w_loader_gr.gif" src="/Content/themes/img/loaders/w_loader_gr.gif" /></span>
            </span>
        </div>
        <div class="clearfix">
        </div>
        <div class="lag">
            <label class="label-160">
                Description</label>
            <span class="field">
                @Html.TextBoxFor(model => model.Description, new { @class = "input input-253" })
                @Html.ValidationMessageFor(model => model.Description)<span class="loading Description">
                    <img title="w_loader_gr.gif" src="/Content/themes/img/loaders/w_loader_gr.gif" /></span>
            </span>
        </div>
        <div class="clearfix">
        </div>
        <div class="lag">
            <label class="label-160">
                Schedule</label>
            <span class="field">
                @Html.TextBoxFor(model => model.Schedule, new { @class = "input input-253" })
                @Html.ValidationMessageFor(model => model.Schedule)
            </span>
        </div>
        
      
        <div class="clearfix">
        </div>
        <div class="lag">
            <label class="label-160">
                Incharge</label>
            <span class="field">
                @Html.TextBoxFor(model => model.Incharge, new { @class = "input input-253" })
                @Html.ValidationMessageFor(model => model.Incharge)
            </span>
        </div>
    </div>

   
}
<script type="text/javascript">
    $(document).ready(function () {
        $("#StartTime, #EndTime").timepicker({
            'timeFormat': 'h:i A',
            'step': 15,
            'minTime': '8:00am',
            'maxTime': '7:00pm',
            'showDuration': true,
            'forceRoundTime': true
        });
    });

    $("#StartTime").live('blur', function () {
        var StartTime = $("#StartTime").val();
        $("#EndTime").timepicker({
            'timeFormat': 'h:i A',
            'step': 15,
            'minTime': StartTime,
            'maxTime': '7:00pm',
            'showDuration': true,
            'forceRoundTime': true
        });
    });
</script>
