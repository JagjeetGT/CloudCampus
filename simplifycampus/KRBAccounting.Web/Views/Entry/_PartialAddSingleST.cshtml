@model KRBAccounting.Web.ViewModels.Entry.BillOfMaterialSingleViewModel
@{
    var trId = "id-" + Model.Id;
}
<tr id="@trId">
    <td>@Model.Code
    </td>
    <td>@Model.Unit
    </td>
    <td>@Model.Qty
    </td>
    <td>@Model.ConversionFactor
    </td>
    <td>@Model.Description
    </td>
    <td>
        <a href="javascript:void(0)" class="STEdit icon-pencil" data-id="@Model.Id" title="Edit: @Model.Code">
        </a>| <a href="javascript:void(0)" class="STRemove icon-remove" data-id="@Model.Id" title="Delete: @Model.Code">
        </a>
    </td>
</tr>
