
@model IEnumerable<KRBAccounting.Domain.Entities.StockTransaction>
<div class="workplace">
    <div class="row-fluid">
        <div class="span12">
            <div class="head">
                <div class="isw-grid">
                </div>
                <h1>
                    Pending</h1>
                <ul class="buttons">
                    <li><a href="#" class="isw-plus addGodownTransfer"></a></li>
                </ul>
                <div class="clear">
                </div>
            </div>
            <div class="block-fluid table-sorting" id="tbl-pending">
                <table cellpadding="0" cellspacing="0" width="100%" class="table tbl-list">
                    <thead>
                        <tr>
                            <th width="20%">
                                Transfer No.
                            </th>
                            <th>
                                Transfer Date
                            </th>
                            <th>
                                Transfer From
                            </th>
                            <th>
                                Quantity
                            </th>
                            <th>
                                Amount
                            </th>
                            <th width="10%">
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.VNo
                                </td>
                                <td>@item.VDate.ToShortDateString()
                                </td>
                                <td>@item.Godown
                                </td>
                                <td>@item.Quantity
                                </td>
                                <td>@item.Amount
                                </td>
                                <td>
                                    <a href="javascript:void(0);" data-id="@item.Id" class="editPurchaseInvoice">Edit</a>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
                @*<div class="pagination">
                    @Html.Raw(Html.AjaxPager(Model, "PurchaseInvoicePendingList", "Entry", new PagerOptions() { PageIndexParameterName = "pageNo", ShowGoButton = false }, new AjaxOptions() { UpdateTargetId = "tbl-pending" }))
                </div>*@
            </div>
        </div>
    </div>
   
</div>
