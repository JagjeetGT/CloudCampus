@model KRBAccounting.Web.ViewModels.Academy.StudentAdmitCardForm
<script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
@using (Ajax.BeginForm("AdmitCardAdd", "Student", new { controller = "Student" }, new AjaxOptions { HttpMethod = "Post", OnBegin = "Begin", OnSuccess = "success" }, new { @class = "form-horizontal from-custom online-form", id = "form-id" }))
{
    
   
    <div class="lag">
        <span class="stack">
            <label class="label-90 ">
                Class</label>
            <span class="field">
                @Html.DropDownListFor(x => x.ClassId, Model.ClassList, new { @Class = "input-190" })
                @Html.ValidationMessageFor(m => m.ClassId)
            </span></span><span class="stack">
                <label class="label-70">
                    Code</label>
                <span class="field">
                    @Html.DropDownListFor(x => x.ExamId, Model.ExamList, new { @Class = "input-190" })
                    @Html.ValidationMessageFor(m => m.ExamId)
                </span></span>
    </div>
    <div class="clearfix">
    </div>
    
    
    

}
