
<div class="workplace">

    <div class="row-fluid">
        <div class="head" style="height: 38px;">
            <h1>
                @ViewBag.Title
            </h1>
            <ul class="buttons">
                <li>
                    <a href="javascript:void(0)" id="generate_report" title="Generate New Report"
                       class="isw-refresh"></a>
                </li>
                <li>
                    <a href="@Url.Action("PdfStudentExtraActivity", "Student")" target="_blank" title="Export to Pdf" class="isw-pdf">
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("ExcelStudentExtraActivity", "Student")" target="_blank" title="Export to Excel" class="isw-excel">
                    </a>
                </li>
                <li><a id="print" title="Print" class="isw-print"></a></li>
            </ul>
            <div class="clear">
            </div>
        </div>
        <div class="row-fluid">
            <div class="block">
                @ViewBag.ErrMsg
            </div>
        </div>
    </div>
</div>
