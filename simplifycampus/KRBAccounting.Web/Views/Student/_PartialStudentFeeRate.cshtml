@model KRBAccounting.Web.ViewModels.Student.StudentGeneralInformationViewModel
           @{
               var data = Model.SpStudentInfoHeader.FirstOrDefault();
           }
<script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
<div class="workplace">
    <div class="row-fluid">
        <div class="span12">
            <div class="head">
                <h1>
                    Student Fee Rate
                </h1>
                <ul class="buttons">
                    <li>
                        <a href="javascript:void(0)" id="generate_report" title="Generate New Report"
                           class="isw-refresh"></a>
                    </li>
                    <li>
                        <a href="@Url.Action("PdfStudentFeeRate", "Student")" target="_blank" title="Export to Pdf" class="isw-pdf">
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("ExcelStudentFeeRate", "Student")" target="_blank" title="Export to Excel" class="isw-excel">
                        </a>
                    </li>
                    <li><a id="print" title="Print" class="isw-print"></a></li>
                </ul>
                <div class="clear">
                </div>
            </div>
            <div class="block-fluid">
    <div class="ReportHeader" style="text-align: center">
        @Html.Partial("_PartialReportHeader", Model.ReportHeader)
     
    </div>
                <br/>
    <div class="block-fluid">
        <p style="font-weight:bold">
            NAME : @data.Student &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
           STUDENT CODE : @data.StdCode &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
            REGISTRATION NO. :  @data.Regno 
            <br />
           
            CLASS : @data.Class &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
            SECTION : @data.Section &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
            ROLL NO. : @data.Rollno 
            <br />
            ADDRESS : @data.TmpAdd,  @data.TmpCity,  @data.TmpDistrict,  @data.TmpCountry

        </p>

        <table cellpadding="0" cellspacing="0" class="listpopup" align="center" >
            <tr >

                <th>Item</th>
                <th>Fee Rate</th>
            </tr>

            @foreach (var item in Model.GetStudentFeeRate)
            {
                <tr >

                    <td align="center">@item.ItemName</td>
                    <td align="center">@item.FeeRate</td>
                </tr>
            }


        </table>
    </div>
</div>

        </div>
    </div>
</div>