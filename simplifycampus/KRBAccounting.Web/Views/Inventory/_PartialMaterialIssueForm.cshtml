@model KRBAccounting.Web.ViewModels.Inventory.InventoryMaterialIssueFormViewModel
<script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
@using (Html.BeginForm("MaterialIssueReport", "Inventory", new { controller = "Inventory" }, FormMethod.Post, new { @class = "form-horizontal", id = "report-form" }))
{
   
        
    <div class="lag">
        
        <label class="label-160">
            Date From(MM-dd-yyyy)</label>
        <span class="field">
            @Html.DateTextBoxFor(x => x.DateFrom, new { @class = "input-100",@placeholder="English Date" })
            @Html.ValidationMessageFor(m => m.DateFrom)
        </span>
        <div class="clear"></div>
        <label class="label-160">
            Date To(MM-dd-yyyyy)</label>
        <span class="field">
            @Html.DateTextBoxFor(x => x.DateTo, new { @class = "input-100",@placeholder="English Date" })
            @Html.ValidationMessageFor(m => m.DateTo)
        </span>
    </div>
       
 
    <div class="clearfix">
    </div>

}
